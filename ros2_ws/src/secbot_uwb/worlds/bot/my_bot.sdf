<?xml version="1.0" ?>
<sdf version="1.9">
    <model name="my_bot">

      <!-- Base Link -->
      <link name="base_link">
        <inertial>
          <pose>0.05 0 0.075 0 0 0</pose>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.0046875</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0046875</iyy>
            <iyz>0</iyz>
            <izz>0.0075</izz>
          </inertia>
        </inertial>

        <!-- Chassis Collision -->
        <collision name="chassis_collision">
          <pose>0.09 0 0.1524 0 0 0</pose>
          <geometry>
            <box>
              <size>0.3048 0.3048 0.3048</size>
            </box>
          </geometry>
        </collision>

        <!-- Chassis Visual -->
        <visual name="chassis_visual">
          <pose>0.09 0 0.1524 0 0 0</pose>
          <geometry>
            <box>
              <size>0.3048 0.3048 0.3048</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
          </material>
        </visual>

        <!-- Camera 1 Visual -->
        <visual name="camera_visual">
          <pose>0.2424 0.140024 0.30 0 0 0</pose>
          <geometry>
            <box>
              <size>0.01 0.03 0.03</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>

        <!-- Camera 2 Visual -->
        <visual name="camera2_visual">
          <pose>0.2424 -0.140024 0.30 0 0 0</pose>
          <geometry>
            <box>
              <size>0.01 0.03 0.03</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>

        <!-- Camera 1 Sensor -->
        <sensor name="camera" type="camera">
          <pose>0.2424 0.140024 0.30 0 0 0</pose>
          <camera>
            <horizontal_fov>1.089</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.05</near>
              <far>8.0</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>10</update_rate>
          <visualize>true</visualize>
          <topic>camera</topic>
        </sensor>

        <!-- Camera 2 Sensor -->
        <sensor name="camera2" type="camera">
          <pose>0.2424 -0.140024 0.30 0 0 0</pose>
          <camera>
            <horizontal_fov>1.089</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.05</near>
              <far>8.0</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>10</update_rate>
          <visualize>true</visualize>
          <topic>camera2</topic>
        </sensor>
      </link>

      <!-- Left Front Wheel -->
      <link name="left_front_wheel">
        <pose relative_to="base_link">0.2 0.17 0 -1.5708 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000174022</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000174022</iyy>
            <iyz>0</iyz>
            <izz>0.000340725</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
      </link>

      <!-- Left Back Wheel -->
      <link name="left_back_wheel">
        <pose relative_to="base_link">-0.02 0.17 0 -1.5708 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000174022</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000174022</iyy>
            <iyz>0</iyz>
            <izz>0.000340725</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
      </link>

      <!-- Right Front Wheel -->
      <link name="right_front_wheel">
        <pose relative_to="base_link">0.2 -0.17 0 1.5708 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000174022</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000174022</iyy>
            <iyz>0</iyz>
            <izz>0.000340725</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.001</mu>
                <mu2>0.001</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
      </link>

      <!-- Right Back Wheel -->
      <link name="right_back_wheel">
        <pose relative_to="base_link">-0.02 -0.17 0 1.5708 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000174022</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.000174022</iyy>
            <iyz>0</iyz>
            <izz>0.000340725</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.08255</radius>
              <length>0.020955</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
          </material>
        </visual>
      </link>

      <!-- Joints -->
      <joint name="left_front_wheel_joint" type="revolute">
        <parent>base_link</parent>
        <child>left_front_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1e16</lower>
            <upper>1e16</upper>
          </limit>
        </axis>
      </joint>

      <joint name="left_back_wheel_joint" type="revolute">
        <parent>base_link</parent>
        <child>left_back_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1e16</lower>
            <upper>1e16</upper>
          </limit>
        </axis>
      </joint>

      <joint name="right_front_wheel_joint" type="revolute">
        <parent>base_link</parent>
        <child>right_front_wheel</child>
        <axis>
          <xyz>0 0 -1</xyz>
          <limit>
            <lower>-1e16</lower>
            <upper>1e16</upper>
          </limit>
        </axis>
      </joint>

      <joint name="right_back_wheel_joint" type="revolute">
        <parent>base_link</parent>
        <child>right_back_wheel</child>
        <axis>
          <xyz>0 0 -1</xyz>
          <limit>
            <lower>-1e16</lower>
            <upper>1e16</upper>
          </limit>
        </axis>
      </joint>

      <!-- Differential Drive Plugin -->
      <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
        <!-- <left_joint>left_front_wheel_joint</left_joint> -->
        <left_joint>left_back_wheel_joint</left_joint>

        <!-- <right_joint>right_front_wheel_joint</right_joint> -->
        <right_joint>right_back_wheel_joint</right_joint>
        <wheel_separation>0.34</wheel_separation>
        <wheel_radius>0.08255</wheel_radius>


        <odom_publish_frequency>50</odom_publish_frequency>
        <topic>/cmd_vel</topic>
        <odom_topic>/odom</odom_topic>
        <frame_id>odom</frame_id>
        <child_frame_id>base_link</child_frame_id>
      </plugin>

      <plugin
        filename="gz-sim-odometry-publisher-system"
        name="gz::sim::systems::OdometryPublisher">
        <odom_publish_frequency>50</odom_publish_frequency>
        <dimensions>3</dimensions>
        <!-- <odom_topic>odom</odom_topic> -->
        <!-- <odom_frame>odomCustom</odom_frame> -->
        <!-- <robot_base_frame>baseCustom</robot_base_frame> -->
      </plugin>
    
    
